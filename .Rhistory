minmaxNormalization <- function(sample,min,max){
if(is.na(sample)){
return(NA)
}
else{
return((sample-min)/(max-min))
}
}
zscoreNormalization <- function(sample,sd,mean){
if(is.na(sample)){
return(NA)
}
else{
return((sample-mean)/(sd))
}
}
